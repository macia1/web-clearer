{
  "manifest_version": 3,
  "name": "clear",
  "description": "Web ui clear toolsï¼Œdelete specified website page source document by document id or class",
  "version": "1.0",
  "icons": {
    "16": "./images/icon-16.png",
    "32": "./images/icon-32.png",
    "128": "./images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ]
    }
  ]
}